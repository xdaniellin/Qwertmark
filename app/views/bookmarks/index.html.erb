            <li class='key 67'>c</li>
            <li class='key 86'>v</li>
            <li class='key 66'>b</li>
            <li class='key 78'>n</li>
            <li class='key 77'>m</li>
            <li class='key 188'>,</li>
          </ul>
          <!--SPACEBAR-->
          <ul class="row">
            <%= link_to 'New Bookmark', new_bookmark_path, class: 'space' %>
          </ul>
          <!--RAILS BOOKMARK LOOP-->
          <% @bookmarks.each do |bookmark| %>
                <div id=<%= "#{bookmark.key}" -%>>
                    <div id="del">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <%= link_to 'x', bookmark, method: :delete, data: { confirm: 'Are you sure?' } %>
                    </div>
                 <div class='bmark' hidden><%= bookmark.url %></div>
                    </br>
                    <%= link_to "  #{bookmark.key.titleize}  ", 'http://' + bookmark.url, id:bookmark.key %>
                </div>
      
    <% end %>


        </div>


     <% else %>
       <p>
         <%= link_to "Log in", new_user_session_path, class: "btn btn-default btn-lg" %>
         <%= link_to "Sign up", new_user_registration_path, class: "btn btn-primary btn-lg" %>
       </p>
  <% end %>  
