<div id="javascript">
     <% if user_signed_in? %>
      <div class="keyboard">
          <ul class="row">
            <li class='key 81'>q</li>
            <li class='key 87'>w</li>
            <li class='key 69'>e</li>
            <li class='key 82'>r</li> 
            <li class='key 84'>t</li>
            <li class='key 89'>y</li>
            <li class='key 85'>u</li>
            <li class='key 73'>i</li>
            <li class='key 79'>o</li>
            <li class='key 80'>p</li>
          </ul>
          <ul class="row">
            <li class='key 65'>a</li>
            <li class='key 83'>s</li>
            <li class='key 68'>d</li>
            <li class='key 70'>f</li>
            <li class='key 71'>g</li>
            <li class='key 72'>h</li>
            <li class='key 74'>j</li>
